{
  "name": "flipboxdigital/http-hubspot",
  "description": "HubSpot PSR7 Http Resources",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "psr",
    "http",
    "guzzle",
    "hubspot"
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\HubSpot\\Http\\": "src/"
    },
    "files": [
      "src/Helpers/Url.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\HubSpot\\Http\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
